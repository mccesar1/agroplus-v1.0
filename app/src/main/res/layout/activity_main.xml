<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/button19"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginBottom="3dp"
        android:backgroundTint="#1C31A8"
        android:onClick="readCSVaquillas"
        android:text="VQS"
        android:textColor="#ffffff"
        android:textSize="43dp"
        app:layout_constraintBottom_toTopOf="@+id/btnDelete"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btnVCS"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginBottom="3dp"
        android:backgroundTint="#1C31A8"
        android:onClick="readCSV"
        android:text="VCS"
        android:textColor="#ffffff"
        android:textFontWeight="900"
        android:textSize="44dp"
        app:layout_constraintBottom_toTopOf="@+id/btnDelete"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginStart="2dp"
        android:onClick="onClick1"
        android:text="1"

        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"
        app:layout_constraintBottom_toTopOf="@+id/btnVCS"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn4"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginStart="2dp"
        android:onClick="onClick4"
        android:text="4"

        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"
        app:layout_constraintBottom_toTopOf="@+id/btn1"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn7"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:onClick="onClick7"
        android:text="7"
        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"

        app:layout_constraintBottom_toTopOf="@+id/btn4"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn5"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:onClick="onClick5"
        android:text="5"
        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"

        app:layout_constraintBottom_toTopOf="@+id/btn2"
        app:layout_constraintEnd_toStartOf="@+id/btn6"
        app:layout_constraintStart_toEndOf="@+id/btn4" />

    <Button
        android:id="@+id/btn8"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:onClick="onClick8"
        android:text="8"

        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"
        app:layout_constraintBottom_toTopOf="@+id/btn5"
        app:layout_constraintEnd_toStartOf="@+id/btn9"
        app:layout_constraintStart_toEndOf="@+id/btn7" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:onClick="onClick2"
        android:text="2"

        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"
        app:layout_constraintBottom_toTopOf="@+id/btn0"
        app:layout_constraintEnd_toStartOf="@+id/btn3"
        app:layout_constraintStart_toEndOf="@+id/btn1" />

    <Button
        android:id="@+id/btn3"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:onClick="onClick3"
        android:text="3"
        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"

        app:layout_constraintBottom_toTopOf="@+id/button19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.996"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:onClick="onClick6"
        android:text="6"

        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"
        app:layout_constraintBottom_toTopOf="@+id/btn3"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btn9"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:onClick="onClick9"
        android:text="9"

        android:textColor="#000000"
        android:textFontWeight="900"
        android:textSize="60dp"
        app:layout_constraintBottom_toTopOf="@+id/btn6"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btn0"
        android:layout_width="148dp"
        android:layout_height="122dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="3dp"
        android:onClick="onClick0"
        android:text="0"

        android:textColor="#000000"
        android:textSize="60dp"
        app:layout_constraintBottom_toTopOf="@+id/btnDelete"
        app:layout_constraintEnd_toStartOf="@+id/button19"
        app:layout_constraintStart_toEndOf="@+id/btnVCS" />

    <Button

        android:id="@+id/btnDelete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:background="#1C31A8"
        android:fontFamily="arial"
        android:onClick="onClickDelete"
        android:text="Borrar"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textSize="65sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.853" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="AGROPLUS"
        android:textColor="#1C31A8"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="ALTA GENETICS"
        android:textColor="#1C31A8"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="73dp"
        android:layout_height="61dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="ID"
        android:textColor="@color/black"
        android:textSize="55sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTxtNum"
        android:layout_width="217dp"
        android:layout_height="71dp"
        android:layout_marginTop="25dp"
        android:background="@android:drawable/editbox_background"
        android:backgroundTint="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="0"
        android:textAlignment="center"
        android:textSize="50dp"
        android:focusable="false"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.572"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:background="#1C31A8"
        android:onClick="onClickDelete"
        android:text="C"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/txtDatos"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:layout_marginTop="104dp"
        android:background="#CDCBCB"
        android:text="Datos"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="30sp"

        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtToros"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_marginTop="140dp"
        android:background="#1C31A8"
        android:text=""
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="#FFFFFF"
        android:textAlignment="textStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:text="Mensajes"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtToros" />

</androidx.constraintlayout.widget.ConstraintLayout>